{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e.spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!(otplib|@otplib|@scure|@noble|qrcode|uuid|node-fetch)/)"
  ],
  "moduleNameMapper": {
    "^uuid$": "<rootDir>/../__mocks__/uuid.js"
  },
  "forceExit": true
}
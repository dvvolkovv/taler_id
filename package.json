  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "node_modules/(?!(otplib|@otplib|@scure|qrcode|uuid|node-fetch)/.*)"
    ],
    "roots": [
      "<rootDir>",
      "<rootDir>/../__mocks__"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }

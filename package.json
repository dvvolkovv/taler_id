    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.19.11",
    "@types/nodemailer": "^7.0.10",
    "@types/passport-jwt": "^4.0.1",
